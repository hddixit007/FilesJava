package day1assessment;

//import java.util.*;

class A 
{
	void show()
	{
		System.out.println("show of A");
	}
}
class B extends A
{
	void show()
	{
		super.show();
		System.out.println("show of B");
	}
}
public class day1assessment {

	public static void main(String args[])
	{
		A a = new A();
		a.show();
		B b = new B();
		b.show();
		
	}
}
