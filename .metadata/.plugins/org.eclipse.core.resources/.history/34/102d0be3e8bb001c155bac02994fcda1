package day1assessment;
import java.util.*;
public class day1assessment {

	public static void main(String args[])
	{
		
		//char[] arr = {'a','b','c','d','e'};
		//char [] ch = "Harshit";      // This is not possible
		
		Set<char> s = new HashSet<char>();
		String s1 = "gama";
		String s2 = "slai";
		int len = s1.length();
		for(int i=0; i<len; i++)
		{
			s.add(s1[i]);
		}
		for(int i=0; i<s2.length(); i++)
		{
			if(s.contains(s2[i]))
			{
				System.out.println(s2[i]);
			}
		}
		
		
		
		
	}
}
