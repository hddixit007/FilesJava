				{3,6,80,21}};
		System.out.println("done");
		
		/*for(int item: arr)
		{
			System.out.println(item);
		}
		
		for(int ele[]: a)
		{
			for(int k: ele)
			{
				System.out.print(k+" ");
			}
			System.out.println();
		}*/
		
		// Variable length arguments
		A a = new A();
		a.calc(10,20,30,40,50,60,70);
		
	}

}
